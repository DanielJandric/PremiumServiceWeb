{
  "version": 2,
  "builds": [
    { "src": "server/index.ts", "use": "@vercel/node" },
    { "src": "client/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)\.(css|js|png|jpg|jpeg|svg|webp|woff2)", "dest": "/dist/public/$1.$2" },
    { "src": "/(.*)", "dest": "/dist/public/index.html" }
  ]
}


